<!-- Hourly script -->
<script>
  $(function() {
    $('.sidebar-menu').tree();
    //iCheck for checkbox and radio inputs
    $('input[type="checkbox"].minimal, input[type="radio"].minimal').iCheck({
      checkboxClass: 'icheckbox_minimal-green',
      radioClass   : 'iradio_minimal-green'
    });

    //datepicker
    //Date picker
    $('#datepicker').datepicker(
        {
            format:'mm-dd-yy',
            autoclose: true,
            endDate: new Date(),
        }).datepicker("setDate",'now');
  });
</script>
<!-- API Response handler -->
<script>
    window.addEventListener('load', function(){

        function goHourly(){

            let xhr = new XMLHttpRequest();
            let fd = new FormData(form); // bind form data object and form element
            
            xhr.addEventListener('load', function(event){
                console.log(event.target.response);
                let resFromServer = JSON.parse(event.target.response);
                document.getElementById('responseDiv').style.display='block';
                function resDiv(){
                    let responseDiv = document.getElementById('responseDiv');
                    
                    if(resFromServer.err){
                        document.getElementById('signinbutton').removeAttribute("disabled");

                        if (responseDiv.classList) { 
                            responseDiv.classList.add("responseDivErr");
                            responseDiv.innerHTML = resFromServer.err
                        } else {
                            // For IE9
                            var classes = responseDiv.className.split(" ");
                            var i = classes.indexOf("responseDivErr");

                            if (i >= 0) {
                                classes.splice(i, 1);
                            } else { 
                                classes.push("responseDivErr");
                                responseDiv.className = classes.join(" "); 
                                responseDiv.innerHTML = resFromServer.err
                            }
                        }

                    } else {

                        if (responseDiv.classList) { 
                            responseDiv.classList.add("responseDivSuccess");
                            responseDiv.innerHTML = resFromServer.auth;
                            
                            let hideDiv = document.getElementById('toHideIfSuccess');
                                hideDiv.style.display = "none";
                                
                            setTimeout(function(){
                                window.location.href="/hourly";

                            }, 10);

                        } else {
                            // For IE9
                            var classes = responseDiv.className.split(" ");
                            var i = classes.indexOf("responseDivSuccess");

                            if (i >= 0) {
                                classes.splice(i, 1);
                            } else { 
                                classes.push("responseDivSuccess");
                                responseDiv.className = classes.join(" "); 
                                responseDiv.innerHTML = resFromServer.auth;
                                
                                let hideDiv = document.getElementById('toHideIfSuccess');
                                    hideDiv.style.display = "none";

                                setTimeout(function(){
                                    window.location.href="/hourly";
                                }, 10);
                            }
                        }
                    }

                    
                }

                resDiv();
            });

            xhr.addEventListener('error', function(event){
                alert('Something went wrong.');
            });

            xhr.open('POST', '/api/hourly');
            xhr.send(fd);

        }

        // access FORM element
        let form = document.getElementById('hourly_form');
        // take over submit event
        form.addEventListener('submit', function(event){
            event.preventDefault();
            goHourly();
            document.getElementById('hourlysubmit').setAttribute("disabled", "disabled");
            document.getElementById('datepicker').setAttribute("disabled", "disabled");
            document.getElementById('shiftall').setAttribute("disabled", "disabled");
            document.getElementById('shiftam').setAttribute("disabled", "disabled");
            document.getElementById('shiftpm').setAttribute("disabled", "disabled");
        });
        
    });
</script>
