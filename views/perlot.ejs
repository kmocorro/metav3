            <div class="row">
                <div class="col-md-12">
                    <div class="box box-default">
                        <div class="box-header with-border">
                            <h3 class="box-title"><%= searched_lot %></h3>
                            <div class="box-tools pull-right">
                                <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
                                <button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-remove"></i></button>
                            </div>
                        </div>
                        <div class="box-body">
                            <table id="perlot" class="perlot-table table-striped table-bordered" >
                                <tr>
                                    <th class="th-format">Created date</th>
                                    <th class="th-format">Line</th>
                                    <th class="th-format">Lot name</th>
                                    <th class="th-format">Lot status</th>
                                    <th class="th-format">Process</th>
                                    <th class="th-format">Start qty</th>
                                    <th class="th-format">Current qty</th>
                                    <th class="th-format-comments">Comments</th>
                                    <th class="th-format">Created by</th>
                                </tr>
                                <% if(lot_history_data) { %>
                                    <% for(let i=0; i<lot_history_data.length;i++) { %>
                                    <tr>
                                    <td class="td-format-process-data"><%= lot_history_data[i].created_date %></td>
                                    <td class="td-format-process-data"><%= lot_history_data[i].line %></td>
                                    <td class="td-format-process-data"><%= lot_history_data[i].lot_name %></td>
                                    <td class="td-format-process-data"><%= lot_history_data[i].status_id %></td>
                                    <td class="td-format-process-data"><%= lot_history_data[i].process_id %></td>
                                    <td class="td-format-process-data"><%= lot_history_data[i].start_qty %></td>
                                    <td class="td-format-process-data"><%= lot_history_data[i].current_qty %></td>
                                    <td class="td-format-process-data"><%= lot_history_data[i].comments %></td>
                                    <td class="td-format-process-data"><%= lot_history_data[i].created_by %></td>
                                    </tr>
                                    <% } %>
                                <% } %>
                            </table>
                        </div>
                        <div class="box-footer">

                        </div>
                    </div>
                </div>
            </div>

