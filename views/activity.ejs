        $('.select2').select2();

        // Daterange_picker
        $('#daterange_picker').daterangepicker({ 
            timePicker: true, 
            timePickerIncrement: 30, 
            locale: {
                format: 'YYYY-MM-DD h:mm A'
            }
        });

        $('#daterange_picker').on('apply.daterangepicker', function(ev, picker) {
            $(this).val(picker.startDate.format('YYYY-MM-DD h:mm A') + ' - ' + picker.endDate.format('YYYY-MM-DD h:mm A'));
        });

        $('#daterange_picker').on('cancel.daterangepicker', function(ev, picker) {
            $(this).val('');
        });

        // Modal Daterange_picker
        <% for(let i=0; i<data.recent.length; i++) { %>
            <% if(data.recent[i].id) { %>
                $('#daterange_picker<%= data.recent[i].id %>').html('<%= data.recent[i].startDate %>' + ' - ' + '<%= data.recent[i].endDate %>');

                $('#daterange_picker<%= data.recent[i].id %>').daterangepicker({
                    startDate: '<%= data.recent[i].startDate %>',
                    endDate: '<%= data.recent[i].endDate %>',
                    timePicker: true, 
                    timePickerIncrement: 30, 
                    locale: {
                        format: 'YYYY-MM-DD h:mm A',
                    }
                });

                $('#daterange_picker<%= data.recent[i].id %>').on('apply.daterangepicker', function(ev, picker) {
                    $(this).val(picker.startDate.format('YYYY-MM-DD h:mm A') + ' - ' + picker.endDate.format('YYYY-MM-DD h:mm A'));
                });

                $('#daterange_picker<%= data.recent[i].id %>').on('cancel.daterangepicker', function(ev, picker) {
                    $(this).val('');
                });

            <% } %>
        <% } %>

    });
</script>
<!-- Upload script -->
<script>
    window.addEventListener('load', function(){

        function goUpload(){

            let xhr = new XMLHttpRequest();
            let fd = new FormData(form); // bind form data object and form element
            
            xhr.addEventListener('load', function(event){
                console.log(event.target.response);
                let resFromServer = JSON.parse(event.target.response);
                document.getElementById('responseDiv').style.display='block';

                function resDiv(){
                    let responseDiv = document.getElementById('responseDiv');
                    
                    if(resFromServer.err){

                        document.getElementById('submitactivity').removeAttribute("disabled");

                        if (responseDiv.classList) { 
                            responseDiv.classList.add("responseDivErr");
                            responseDiv.innerHTML = resFromServer.err
                        } else {
                            // For IE9
                            var classes = responseDiv.className.split(" ");
                            var i = classes.indexOf("responseDivErr");

                            if (i >= 0) {
                                classes.splice(i, 1);
                            } else { 
                                classes.push("responseDivErr");
                                responseDiv.className = classes.join(" "); 
                                responseDiv.innerHTML = resFromServer.err
                            }
                        }

                    } else {

                        if (responseDiv.classList) { 
                            responseDiv.classList.add("responseDivSuccess");
                            responseDiv.innerHTML = resFromServer.auth;

                            
                            setTimeout(function(){
                                window.location.href="/activity";
                            }, 50);

                        } else {
                            // For IE9
                            var classes = responseDiv.className.split(" ");
                            var i = classes.indexOf("responseDivSuccess");

                            if (i >= 0) {
                                classes.splice(i, 1);
                            } else { 
                                classes.push("responseDivSuccess");
                                responseDiv.className = classes.join(" "); 
                                responseDiv.innerHTML = resFromServer.auth;
                                
                                setTimeout(function(){
                                    window.location.href="/activity";
                                }, 50);
                            }
                        }
                    }

                    
                }

                resDiv();
            });

            xhr.addEventListener('error', function(event){
                alert('Something went wrong.');
            });

            xhr.open('POST', '/api/activity');
            xhr.send(fd);

        }

        // access FORM element
        let form = document.getElementById('uploadactivity');
        // take over submit event
        form.addEventListener('submit', function(event){
            event.preventDefault();
            //document.getElementById('responseDiv').style.display="none";
            goUpload();
            document.getElementById('submitactivity').setAttribute("disabled", "disabled");

        });


    });
</script>
