            Engineering Activity
            <small>prototype</small>
            <small><label class="label label-success">active</label></small>
        </h1>
        <ol class="breadcrumb">
            <li class="active"><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
        </ol>
        </section>

        <!-- Main content -->
            <section class="content">
                <div class="whitey" style="background: white";>
                    <div class="col-md-12 col-centered">
                        <div class="row">
                            <div class="col-md-4">
                                <form id="uploadactivity">
                                    <div id="toHideIfSuccess">
                                        <input type="hidden" name="authenticity_token" value="<%= authenticity_token %>">
                                        <h4 class="coa-body-form">Activities & experiments</h4>
                                        <label class="coa-form-label">Datetime range:<span class="text-danger">*</span></label>
                                        <div class="form-group">
                                            <div class="input-group">
                                                <div class="input-group-addon">
                                                    <i class="fa fa-calendar"></i>
                                                </div>
                                                <input type="text" name="daterange" class="form-control daterangepicker-dropdown" id="daterange_picker">
                                            </div>
                                        </div>
                                        <label class="coa-form-label">Activity Name:<span class="text-danger">*</span></label>
                                        <div class="form-group has-feedback">
                                            <input type="text" name="activity_title" id="activity_title" class="form-control" required pattern=".{0}|.{5,20}" title="Activity name should be atleast 5 to 20 characters." onchange="document.getElementById('responseDiv').style.display = 'none';">
                                        </div>
                                        <label class="coa-form-label">TDN #: <small><span class="form-sub-text">leave blank if not applicable</span></small></label>
                                        <div class="form-group ">
                                            <input type="text" name="tdn_no" id="tdn_no" class="form-control" onchange="document.getElementById('responseDiv').style.display = 'none';">
                                        </div>
                                        <label class="coa-form-label">MRB #: <small><span class="form-sub-text">leave blank if not applicable</span></small></label>
                                        <div class="form-group ">
                                            <input type="text" name="mrb_no" id="mrb_no" class="form-control" onchange="document.getElementById('responseDiv').style.display = 'none';">
                                        </div>
                                        <label class="coa-form-label">EC #: <small><span class="form-sub-text">leave blank if not applicable</span></small></label>
                                        <div class="form-group">
                                            <input type="text" name="ec_no" id="ec_no" class="form-control" onchange="document.getElementById('responseDiv').style.display = 'none';">
                                        </div>
                                        <label class="coa-form-label">Type:<span class="text-danger">*</span></label>
                                        <div class="form-group">
                                            <select class="form-control select2 select2-hidden-accessible" name="activity_type" style="width:100%;" tabindex="-1" aria-hidden="true">
                                                <option>STO</option>
                                                <option>COL</option>
                                                <option>FTO</option>
                                            </select>
                                        </div>
                                        <label class="coa-form-label">Process:<span class="text-danger">*</span></label>
                                        <div class="form-group has-feedback">
                                            <select class="form-control select2 select2-hidden-accessible" name="process_name" style="width:100%;" tabindex="-1" aria-hidden="true">
                                                <% for(let i=0; i<process_list.length;i++) { %>
                                                    <option><%=  process_list[i] %></option>
                                                <% } %>
                                            </select>
                                        </div>
                                        <label class="coa-form-label">Description:<span class="text-danger">*</span> <small><span class="form-sub-text">160 max characters</span></small></label>
                                        <div class="form-group has-feedback">
                                            <textarea rows="2" maxlength="160" name="activity_details" id="activity_details" class="form-control" required onchange="document.getElementById('responseDiv').style.display = 'none';"></textarea>
                                        </div>
                                        <br>
                                        <div id="responseDiv">
                                            <!-- response here-->
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-7">
                                                <div class="coa-sub-link">
                                                    By clicking "Submit", you indicate that all fields are true and correct. Having trouble posting? Kindly use Chrome or Mozilla.
                                                </div>
                                            </div>
                                            <div class="col-xs-5">
                                                <button type="submit" id="submitactivity" class="btn btn-primary btn-block btn-flat">Post</button>
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                            <div class="col-md-8">
                                <h4 class="coa-body-form">Contribution activity</h4>
                                <ul class="timeline timeline">
                                    <% for(let i=0; i<data.recent.length; i++) { %>
                                        <% if(data.recent[i].id) { %>
                                            <li>
                                                <i class="fa fa-cloud-upload bg-gray">
                                                </i>
                                                <div class="timeline-item">
                                                    <span class="time">
                                                        ends <%= data.recent[i].timeLeft %>&nbsp;
                                                        <button type="button" class="btn btn-link btn-flat dropdown-toggle" data-toggle="dropdown">
                                                            <span class="fa fa-ellipsis-v" style="color:slategray"></span>
                                                        </button>
                                                        <ul class="dropdown-menu coa-dropdown" role="menu">
                                                            <li>
                                                                <a href="#edit" data-toggle="modal" data-target="#modalwarning<%= data.recent[i].id %>"><i class="fa fa-pencil"></i>Edit activity</a>
                                                            </li>
                                                            <li class="divider"></li>
                                                            <li>
                                                                <a href="#delete" data-toggle="modal" data-target="#modaldelete<%= data.recent[i].id %>"><i class="fa fa-trash"></i>Delete activity</a>
                                                            </li>
                                                        </ul>
                                                    </span>
                                                    <h3 class=" recent-activity-timeline-header timeline-header"><%= data.recent[i].activity_title %></h3>
                                                    <div class="timeline-body recent-activity-timeline-body"><%= data.recent[i].activity_details %></div>
                                                    <div class="timeline-footer">
                                                    <span class="footer-timeline">
                                                        <div style="overflow-x:auto;">
                                                            <table>
                                                                <tr style="font-size:12px;" >
                                                                    <td style="padding:0 5px 0;"><%= data.recent[i].upload_date %></td>
                                                                    <td style="padding:0 5px 0;"><%= data.recent[i].activity_type %></td>
                                                                    <td style="padding:0 5px 0;"><%= data.recent[i].process_name %></td>
                                                                    <td style="padding:0 5px 0;"><%= data.recent[i].tdn_no  || '' %></td>
                                                                    <td style="padding:0 5px 0;"><%= data.recent[i].mrb_no || '' %></td>
                                                                    <td style="padding:0 5px 0;"><%= data.recent[i].ec_no || '' %></td>
                                                                </tr>
                                                            </table>
                                                        </div>
                                                    </div>
                                                </div>
                                            </li>
                                        <% } else { %>
                                            <li>
                                                <i class="fa fa-meh-o bg-gray">
                                                </i>
                                                <div class="timeline-item">
                                                    <h3 class=" recent-activity-timeline-header timeline-header">You don't have any posts yet.</h3>
                                                </div>
                                            </li>
                                        <% } %>
                                    <% } %>
                                    <li>
                                        <i class="fa fa-clock-o bg-gray"></i>
                                    </li>
                                </ul>
                            </div>
