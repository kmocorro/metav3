                            <% for(let i=0; i<data.recent.length; i++) { %>
                                <% if(data.recent[i].id) { %>
                                <div class="modal modal-default" id="modaldelete<%= data.recent[i].id %>">
                                    <div class="modal-dialog modal-settings">
                                        <div class="modal-content">
