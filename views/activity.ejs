                                                    <input type="hidden" name="deleteByUsername" value="<%= data.recent[i].username %>">
                                                    <div id="responseDivModal<%= data.recent[i].id %>">
                                                        <!-- response here-->
                                                    </div>
                                                    <div id="toHideButtonIfSuccess<%= data.recent[i].id %>">
                                                        <button type="button" class="btn btn-default btn-flat " data-dismiss="modal" aria-label="Close" >Cancel</button>
                                                        <button type="submit" id="deletebyactivity<%= data.recent[i].id %>" class="btn btn-danger btn-flat">Yes, Delete</button>
                                                    </div>
                                                </form>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <% } %>
                            <% } %>
